{
  "buildCommand": "echo 'No build needed - static files'",
  "outputDirectory": "frontend/public",
  "framework": null,
  "installCommand": "echo 'No install needed'",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        }
      ]
    }
  ]
}
