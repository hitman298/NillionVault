# Upstash Redis Configuration
UPSTASH_REDIS_REST_URL=https://[region]-[project].upstash.io
UPSTASH_REDIS_REST_TOKEN=[redis-token]

# CORS Configuration
FRONTEND_URL=http://localhost:3000
